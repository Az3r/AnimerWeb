rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {

    match /{document=**} {
      allow read, write: if false;
    }

    match /authorization/{auth} {
      allow read: if true;
      allow create: if true;
      allow update: if !resource.data.keys().hasAny(['access_token']);
    }
  }
}